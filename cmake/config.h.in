#ifndef _CONFIG_H
#define _CONFIG_H

/**
\file config.h
\author	 Lukas Reichmann
\since   1.0.0.0
\version @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@.@PROJECT_VERSION_TWEAK@
\brief   This file will be configured by CMake and provides macros that inject CMake defined variables into 
		 the source code of the project.
*/

/**
\brief
An integer constant that expands to the major part of the current version of the library.

\note
This constant should not be used by a user of the library, it is only made for internal usage.

\author  Lukas Reichmann
\version @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@.@PROJECT_VERSION_TWEAK@
\since   1.0.0.0
*/
#ifndef _INTERNAL_VERSION_MAJOR
#define _INTERNAL_VERSION_MAJOR @PROJECT_VERSION_MAJOR@
#endif

/**
\brief
An integer constant that expands to the minor part of the current version of the library.

\note
This constant should not be used by a user of the library, it is only made for internal usage.

\author  Lukas Reichmann
\version @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@.@PROJECT_VERSION_TWEAK@
\since   1.0.0.0
*/
#ifndef _INTERNAL_VERSION_MINOR
#define _INTERNAL_VERSION_MINOR @PROJECT_VERSION_MINOR@
#endif

/**
\brief
An integer constant that expands to the patch part of the current version of the library.

\note
This constant should not be used by a user of the library, it is only made for internal usage.

\author  Lukas Reichmann
\version @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@.@PROJECT_VERSION_TWEAK@
\since   1.0.0.0
*/
#ifndef _INTERNAL_VERSION_PATCH
#define _INTERNAL_VERSION_PATCH @PROJECT_VERSION_PATCH@
#endif

/**
\brief
An integer constant that expands to the tweak part of the current version of the library.

\note
This constant should not be used by a user of the library, it is only made for internal usage.

\author  Lukas Reichmann
\version @PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@.@PROJECT_VERSION_TWEAK@
\since   1.0.0.0
*/
#ifndef _INTERNAL_VERSION_TWEAK
#define _INTERNAL_VERSION_TWEAK @PROJECT_VERSION_TWEAK@
#endif

#endif
